<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="eBayAPISoapBinding">
                    <security mode="Transport" />
                </binding>
                <binding name="eBayAPISoapBinding1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://api.ebay.com/wsapi" binding="basicHttpBinding"
                bindingConfiguration="eBayAPISoapBinding" contract="EbaySoapService.eBayAPIInterface"
                name="eBayAPI" />
        </client>
    </system.serviceModel>
</configuration>